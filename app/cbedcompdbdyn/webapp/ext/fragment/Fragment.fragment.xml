				<Label id="ordercanbe" text=" Order can be given to……..." visible="false"/>
				<Label id="tech_approved" text=" Techncially Approved " class="custtechnically" visible="false"/>
				<Label id="approvedvendor" text="Approved Vendor" class="custtechnically" visible="false"/>

			</VBox>


		</VBox>
		<ScrollContainer id="rightScrollcontainer" class="scroll-container" width="100%" horizontal= "true">
			<HBox id="rightHboxcontainer" class="custBackgroundcolor" width="70vw" wrap="NoWrap">

			</HBox>
		</ScrollContainer>
