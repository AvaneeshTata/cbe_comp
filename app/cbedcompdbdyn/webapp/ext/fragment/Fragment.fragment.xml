					<core:Icon id="icon3" src="sap-icon://navigation-right-arrow" color="darkblue" hoverColor="red" activeColor="darkgreen" width="20px" core:require="{ handler: 'cbedcompdbdyn/ext/fragment/Fragment'}" press="handler.onPress_priccebasis" size="13px"></core:Icon>
				</HBox>
				<Label id="pricesbasis" text="PriceBasis" visible="false"/>
				<Label id="point_of_delivery" text="Point of Delivery" visible="false"/>
				<Label id="del_period" text="Delivery Period" visible="false"/>
				<Label id="payment_terms" text="Payment Terms" class="custompayment" visible="false"/>
				<Label id="liq_dam" text="Liquidated Damages" class="customliquidated" visible="false"/>
